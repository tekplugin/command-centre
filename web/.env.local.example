# Bank API Configuration
# Replace these with your actual bank API credentials

# Bearer token for bank API authentication
NEXT_PUBLIC_BANK_API_TOKEN=your_bearer_token_here

# Client secret for encryption
NEXT_PUBLIC_BANK_CLIENT_SECRET=your_client_secret_here

# Consent token (AES-256-CBC encrypted)
NEXT_PUBLIC_BANK_CONSENT_TOKEN=your_consent_token_here

# Your company bank account number
NEXT_PUBLIC_BANK_ACCOUNT_NUMBER=0123456789
